{
  "name": "plain-jane",
  "version": "1.0.0",
  "main": "build/main.js",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/server/main.js",
    "dev": "npm-run-all --parallel dev:client dev:server",
    "dev:server": "ts-node-dev server/main.ts",
    "dev:client": "parcel watch client/index.html -d dist/client --public-url /dist/",
    "build": "npm-run-all build:server build:client",
    "build:server": "cross-env NODE_ENV=production tsc",
    "build:client": "cross-env NODE_ENV=production parcel build client/index.html -d dist/client --public-url /dist/",
    "lint": "eslint --fix \"**/*.{ts,tsx}\"",
    "depcheck": "depcheck --ignores=\"@types/*,body-parser,pg,ts-node,depcheck\" --ignore-patterns=build/*,node_modules"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.5",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "minio": "^7.0.18",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "pg": "^8.5.1",
    "random-words": "^1.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redis": "^3.0.2",
    "swr": "^0.4.1",
    "typeorm": "^0.2.30",
    "uuid": "^8.3.2",
    "yn": "^4.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cheerio": "^0.22.23",
    "@types/connect-redis": "^0.0.16",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/minio": "^7.0.7",
    "@types/node": "^14.14.22",
    "@types/node-fetch": "^2.5.8",
    "@types/passport": "^1.0.5",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/random-words": "^1.1.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/redis": "^2.8.28",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "cross-env": "^7.0.3",
    "depcheck": "^1.3.1",
    "eslint": "^7.18.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
}
